<% if logged_in? %>
  <% if current_user.role? :admin %>
    <%= render :partial => 'admin_home' %>
  <% elsif current_user.role? :guardian %>
    <%= render :partial => 'parent_home' %>
    <%= render :partial => 'logout' %>
  <% elsif current_user.role? :volunteer %>
    <%# if current_user.volunteer.role == "Coach" or current_user.volunteer.role == "Assistant Coach" %>
      <%# render :partial => 'coach_home' %>
    <%# else %>
      <%= render :partial => 'volunteer_home' %>
    <%# end %>
    <%= render :partial => 'logout' %>
  <% else %>
    <%= render :partial => 'logout' %>
  <% end %>
<% else %>
  <%= render :partial => 'general_home' %>
<% end %>